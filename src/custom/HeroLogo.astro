<svg width="300" height="100" xmlns="http://www.w3.org/2000/svg"><text x="50%" y="50%" font-size="60" fill="#ff9900" font-family="STXingkai" text-anchor="middle" dominant-baseline="middle">喵落阁</text></svg>
  <style>
    #logo {
      animation: logo__to 3000ms forwards cubic-bezier(0.165, 0.84, 0.44, 1);
    }

    #yin-1,
    #he-2,
    #du-2 {
      animation: logoSlideUp 3000ms forwards cubic-bezier(0.165, 0.84, 0.44, 1)
    }

    #yin-2,
    #he-1,
    #du-1,
    #zhou-1 {
      animation: logoSlideDown 3000ms forwards cubic-bezier(0.165, 0.84, 0.44, 1)
    }

    #yin-3 {
      animation: yin-3__to 3000ms forwards cubic-bezier(0.165, 0.84, 0.44, 1)
    }

    #zhou-2 {
      animation: zhou-2__to 3000ms forwards cubic-bezier(0.165, 0.84, 0.44, 1)
    }

    @keyframes logo__to {
      0% {
        opacity: 0
      }

      16% {
        opacity: 0
      }

      33% {
        opacity: 1
      }

      100% {
        opacity: 1
      }
    }

    @keyframes logoSlideDown {
      0% {
        transform: translateY(-50%)
      }

      16% {
        transform: translateY(-50%)
      }

      33% {
        transform: translateY(0)
      }

      100% {
        transform: translateY(0)
      }
    }

    @keyframes logoSlideUp {
      0% {
        transform: translateY(50%)
      }

      16% {
        transform: translateY(50%)
      }

      33% {
        transform: translateY(0)
      }

      100% {
        transform: translateY(0)
      }
    }

    @keyframes yin-3__to {
      0% {
        transform: translate(-100%, 100%)
      }

      23% {
        transform: translate(-100%, 100%);
      }

      40% {
        transform: translate(0, 0)
      }

      100% {
        transform: translate(0, 0)
      }
    }

    @keyframes zhou-2__to {
      0% {
        transform: translate(100%, -100%)
      }

      23% {
        transform: translate(100%, -100%)
      }

      40% {
        transform: translate(0, 0)
      }

      100% {
        transform: translate(0, 0)
      }
    }
  </style>
</svg>